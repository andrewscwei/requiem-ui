{
  "name": "requiem-ui",
  "version": "0.2.0",
  "description": "UI element extension to Requiem, an experimental front-end JS framework.",
  "homepage": "http://andrewscwei.github.io/requiem-ui",
  "license": "MIT",
  "main": "lib/requiem-ui.js",
  "scripts": {
    "start": "npm run build",
    "test": "mocha tests --recursive --reporter spec --compilers js:babel-register",
    "clean": "rimraf lib",
    "build": "npm run clean && ncp src lib",
    "docs": "rimraf docs && jsdoc src -r -R README.md -c .docconfig",
    "play": "rimraf playground/public && pug -O \"{basedir:'playground/app/templates'}\" -P -w -o playground/public playground/app/templates/index.pug & webpack-dev-server --progress --inline --hot --config playground/webpack.config.babel.js --content-base playground/public --port 8080"
  },
  "authors": [
    "Andrew Wei <andrew@andr.mu>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewscwei/requiem.git"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "css-loader": "^0.23.1",
    "jsdoc": "^3.4.0",
    "minuet": "^0.13.0",
    "mocha": "^3.0.2",
    "ncp": "^2.0.0",
    "node-sass": "^3.8.0",
    "pug": "^2.0.0-beta4",
    "pug-cli": "^1.0.0-alpha6",
    "pug-loader": "^2.2.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webcomponents.js": "^0.7.22",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": "6.2.0"
  },
  "dependencies": {
    "requiem": "^0.55.0"
  }
}
